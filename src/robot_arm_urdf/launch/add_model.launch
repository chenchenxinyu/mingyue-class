<launch>
    <rosparam file="$(find robot_arm_urdf)/config/robot_arm_four.yaml" command="load"/>
    <param name="robot_arm"
     textfile="$(find robot_arm_urdf)/urdf/robot_arm_description.urdf"/>
    <!--<include file="$(find gazebo_ros)/launch/empty_world.launch"/>-->

    <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model"  
       args="-param robot_arm -urdf -model panda" />
    <!--start the controller plus-ins-->
    <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" 
        output="screen" ns="/panda" args="joint_state_controller joint1_position_controller"/>
</launch>
